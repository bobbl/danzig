# Copy this file to /etc/udec/rules.d/ to enable non-rooot access to the USB
# programming and UART connection of all FPGAs supported by Danzig

# Lattice
ATTRS{idVendor}=="0403", ATTRS{idProduct}=="6010", MODE="0660", GROUP="plugdev", TAG+="uaccess"
ATTRS{idVendor}=="0403", ATTRS{idProduct}=="6014", MODE="0660", GROUP="plugdev", TAG+="uaccess"

# Genesys2 (Xilinx)
ATTRS{idVendor}=="0403", ATTRS{idProduct}=="6001", MODE="0660", GROUP="plugdev", TAG+="uaccess"
